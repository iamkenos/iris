{
  "name": "@iamkenos/iris",
  "version": "0.0.0",
  "description": "An API testing tool using Axios and Jest",
  "author": "iamkenos",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:iamkenos/iris.git"
  },
  "scripts": {
    "prepare": "husky install",
    "release:first": "standard-version --first-release",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "lint": "eslint ./",
    "prebuild": "rimraf build && rimraf *.tgz",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "resolve-tspaths",
    "test:demo": "jest --config demo/jest.config.ts"
  },
  "license": "ISC",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./build/client/index.d.ts"
      ],
      "common": [
        "./build/common/index.d.ts"
      ],
      "config": [
        "./build/config/index.d.ts"
      ],
      "matchers": [
        "./build/matchers/index.d.ts"
      ],
      "steps": [
        "./build/steps/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./client": {
      "import": "./src/client/index.ts",
      "require": "./build/client/index.js"
    },
    "./common": {
      "import": "./src/common/index.ts",
      "require": "./build/common/index.js"
    },
    "./config": {
      "import": "./src/config/index.ts",
      "require": "./build/config/index.js"
    },
    "./matchers": {
      "import": "./src/matchers/index.ts",
      "require": "./build/matchers/index.js"
    },
    "./steps": {
      "import": "./src/steps/index.ts",
      "require": "./build/steps/index.js"
    }
  },
  "dependencies": {
    "@types/jest": "^27.0.0",
    "ajv": "^8.11.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "allure-commandline": "^2.20.1",
    "axios": "^0.26.0",
    "callsites": "^3.1.0",
    "change-case": "^4.1.2",
    "jest": "^27.0.0",
    "jest-allure": "0.1.3",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "husky": "^7.0.4",
    "resolve-tspaths": "^0.3.0",
    "standard-version": "^9.3.2"
  }
}
